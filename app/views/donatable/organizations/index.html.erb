<p id="notice"><%= notice %></p>

<h1>Organizations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Website</th>
      <th>Twitter</th>
      <th>Facebook</th>
      <th>Phone</th>
      <th>City</th>
      <th>State or district</th>
      <th>Country</th>
      <th>Banner url</th>
      <th>Logo url</th>
      <th>Short description</th>
      <th>Long description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @organizations.each do |organization| %>
      <tr>
        <td><%= organization.name %></td>
        <td><%= organization.website %></td>
        <td><%= organization.twitter %></td>
        <td><%= organization.facebook %></td>
        <td><%= organization.phone %></td>
        <td><%= organization.city %></td>
        <td><%= organization.state_or_district %></td>
        <td><%= organization.country %></td>
        <td><%= organization.banner_url %></td>
        <td><%= organization.logo_url %></td>
        <td><%= organization.short_description %></td>
        <td><%= organization.long_description %></td>
        <td><%= link_to 'Show', organization %></td>
        <td><%= link_to 'Edit', edit_organization_path(organization) %></td>
        <td><%= link_to 'Destroy', organization, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Organization', new_organization_path %>
